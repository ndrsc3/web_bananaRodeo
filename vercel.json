{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "packages/client/dist",
  "functions": {
    "api/routes/**/*.js": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "routes": [
    {
      "src": "/api/hit-counter(.*)",
      "dest": "/api/routes/hit-counter$1"
    },
    {
      "src": "/api/kv(.*)",
      "dest": "/api/routes/kv$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 